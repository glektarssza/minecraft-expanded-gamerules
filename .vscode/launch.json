{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Client (Forge)",
            "request": "launch",
            "mainClass": "net.minecraftforge.userdev.LaunchTesting",
            "projectName": "forge",
            "cwd": "${workspaceFolder}\\forge\\run",
            "vmArgs": "-Dmixin.env.remapRefMap\u003dtrue -Dmixin.env.refMapRemappingFile\u003d${workspaceFolder}\\forge/build/createSrgToMcp/output.srg -Dnet.minecraftforge.gradle.GradleStart.srg.srg-mcp\u003d${workspaceFolder}\\forge\\build\\createSrgToMcp\\output.srg -XX:+IgnoreUnrecognizedVMOptions --add-exports\u003djava.base/sun.security.util\u003dALL-UNNAMED --add-exports\u003djdk.naming.dns/com.sun.jndi.dns\u003djava.naming --add-opens\u003djava.base/java.util.jar\u003dALL-UNNAMED -XX:HeapDumpPath\u003dMojangTricksIntelDriversForPerformance_javaw.exe_minecraft.exe.heapdump \"-Dos.name\u003dWindows 10\" -Dos.version\u003d10.0",
            "args": "--mixin.config expandedgamerules.mixins.json",
            "env": {
                "MOD_CLASSES": "modClientRun%%${workspaceFolder}\\forge\\build\\resources\\main;modClientRun%%${workspaceFolder}\\forge\\build\\classes\\java\\main",
                "MCP_MAPPINGS": "official_1.16.5",
                "MCP_VERSION": "20210115.111550",
                "FORGE_VERSION": "36.2.42",
                "assetIndex": "1.16",
                "assetDirectory": "${env:HOME}\\scoop\\apps\\gradle\\current\\.gradle\\caches\\forge_gradle\\assets",
                "nativesDirectory": "${workspaceFolder}\\forge\\build\\natives",
                "FORGE_GROUP": "net.minecraftforge",
                "target": "fmluserdevclient",
                "MC_VERSION": "1.16.5"
            },
            "preLaunchTask": "forge:prepareClientCompile"
        },
